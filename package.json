{
  "name": "awesomeproject",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|expo-camera| @expo/vector-icons| @react-navigation/bottom-tabs| @expo/vector-icons )"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^0.17.0",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.16",
    "eas-cli": "^2.1.0",
    "expo": "^47.0.8",
    "expo-camera": "~12.3.0",
    "expo-image-picker": "latest",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "get-pixels": "^3.3.3",
    "hex-color-to-color-name": "^1.0.2",
    "jest": "^26.6.3",
    "jest-expo": "^47.0.1",
    "react": "18.1.0",
    "react-dom": "^18.2.0",
    "react-native": "0.70.5",
    "react-native-color-picker": "0.4.3",
    "react-native-get-pixel-color": "^1.1.0-alpha.23",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-snap-carousel": "^4.0.0-beta.6",
    "react-test-renderer": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/react-native": "^11.5.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-universe": "^11.1.1",
    "prettier": "^2.8.1"
  },
  "private": true
}
